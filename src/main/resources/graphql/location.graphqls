schema {
    query: Query
    mutation: Mutation
}

type Location{
    idLocation: ID!
    city: String
}

type Query{
    getLocations: [Location]
    getLocation(id: ID!): Location
}

type Mutation{
    newLocation(city: String): Location
    updateLocation(id: ID!, city: String): Location
    deleteLocation(id: ID!): Boolean
}
